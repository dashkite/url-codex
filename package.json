{
  "name": "@dashkite/url-codex",
  "version": "0.1.0",
  "description": "Simple JSON Query-based text interpolation for JavaScript",
  "exports": {
    ".": {
      "browser": "./build/browser/src/index.js",
      "node": "./build/node/src/index.js"
    },
    "./*": {
      "browser": "./build/browser/src/*.js",
      "node": "./build/node/src/*.js"
    },
    "./parsers": {
      "browser": "./build/browser/src/parsers/index.js",
      "node": "./build/node/src/parsers/index.js"
    }
  },
  "scripts": {
    "test": "genie test",
    "build": "genie build"
  },
  "files": [
    "build/node/src",
    "build/browser/src"
  ],
"repository": {
    "type": "git",
    "url": "git+https://github.com/dashkite/url-codex.git"
  },
  "bugs": {
    "url": "https://github.com/dashkite/url-codex/issues"
  },
  "homepage": "https://github.com/dashkite/url-codex#readme",
  "devDependencies": {
    "@dashkite/amen": "^0.1.2",
    "@dashkite/amen-console": "^0.0.2",
    "@dashkite/assert": "^0.1.3",
    "@dashkite/genie": "link:../genie",
    "@dashkite/genie-coffee": "link:../genie-coffee",
    "@dashkite/genie-yaml": "link:../genie-yaml"
  },
  "dependencies": {
    "@dashkite/joy": "^0.3.16",
    "@dashkite/parse": "^0.4.3"
  }
}
